<?xml version="1.0"?>
<deployment>
    <cluster hostcount="count" sitesperhost="sites" kfactor="kfactor" />
    <httpd enabled="true">
        <jsonapi enabled="true" />
      </httpd>
      <arieslog enabled="true" logsize="6144" >
	<frequency time="freq" />
